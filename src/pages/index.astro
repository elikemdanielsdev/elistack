---
import Layout from "@/layouts/MainLayout.astro";
import { HeadingText, BodyText, Section, Container } from "@/components/static";

const title = "Introduction";
const description = "A minimal Astro starter for building websites.";
const keywords = ["astro", "astro starter", "astro template"];
---

<Layout title={title} description={description} keywords={keywords}>
  <Section>
    <Container>
      <HeadingText tag="h1">{title}</HeadingText>
      <BodyText>A minimal Astro starter for building websites.</BodyText>
    </Container>
  </Section>
</Layout>
